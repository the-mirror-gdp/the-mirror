    "migrate:create": "migrate-mongo create",
    "migrate:up": "migrate-mongo up",
    "migrate:down": "migrate-mongo down",
    "migrate:status": "migrate-mongo status",
    "start": "yarn start:prod",
    "start:debug": "nest start --debug --watch",
    "dev": "yarn prebuild && nest start --watch --debug",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --quiet --fix",
    "lint:ci": "eslint \"{src,apps,libs,test}/**/*.ts\" --quiet",
    "prettier": "pretty-quick",
    "test": "vitest",
    "test:ci": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest --coverage",
    "test:debug": "vitest --inspect-brk --inspect --logHeapUsage --threads=false",
    "test:int": "vitest --config ./vitest.integration-config.ts",
    "test:int:ci": "vitest run --config ./vitest.integration-config.ts",
    "test:e2e": "vitest --config ./vitest.e2e-config.ts",
    "test:e2e:ci": "vitest run --config ./vitest.e2e-config.ts"
  },
