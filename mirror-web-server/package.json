    "gcp-build": "yarn prebuild && yarn build",
    "swagger:generator": "node node_modules/@openapitools/openapi-generator-cli/main.js",
    "swagger:generate": "yarn swagger:generator -- generate -i https://enter.themirror.world/version-test/api/1.1/meta/swagger.json -o src/external/bubble/openapi-client -g typescript-axios",
